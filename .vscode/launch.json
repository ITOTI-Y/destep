    "version": "0.2.0",
    "configurations": [
        {
            "name": "Extract",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": ["extract", "--accdb", "${workspaceFolder}/examples/LH_Guangzhou_2015.accdb", "--output", "${workspaceFolder}/output/destep.sqlite", "--driver", "${workspaceFolder}/driver"]
        },
        {
            "name": "Code Generation",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": ["codegen"]
        },
        {
            "name": "Convert",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": ["convert", "--output-path", "${workspaceFolder}/output/destep.idf", "--sqlite-path", "${workspaceFolder}/output/destep.sqlite"]
        },
    ]
}